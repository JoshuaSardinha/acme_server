# Sample Development Environment Configuration

# Environment
NODE_ENV=development
ENVIRONMENT=dev

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=acme_db
DB_USER=acme_user
DB_PASSWORD=YOUR_PASSWORD

# Test Database Configuration
TEST_DB_HOST=localhost
TEST_DB_PORT=3306
TEST_DB_NAME=acme_test_db
TEST_DB_USER=acme_user_test
TEST_DB_PASSWORD=YOUR_PASSWORD_TEST

# Auth0 Configuration
AUTH0_ISSUER_BASE_URL=YOUR_AUTH0_ISSUER_BASE_URL
AUTH0_CLIENT_ID=YOUR_AUTH0_CLIENT_ID
AUTH0_AUDIENCE=YOUR_AUTH0_AUDIENCE
AUTH0_CLIENT_SECRET=YOUR_AUTH0_CLIENT_SECRET
AUTH0_MANAGEMENT_CLIENT_SECRET=YOUR_AUTH0_MANAGEMENT_CLIENT_SECRET
AUTH0_MANAGEMENT_CLIENT_ID=YOUR_AUTH0_MANAGEMENT_CLIENT_ID
AUTH0_MANAGEMENT_AUDIENCE=YOUR_AUTH0_MANAGEMENT_AUDIENCE

# Client Version Requirements
MIN_IOS_VERSION=1.0.0
MIN_ANDROID_VERSION=1.0.0
MIN_WEB_VERSION=1.0.0

# Feature Flags
ENABLE_DEBUG_LOGGING=true
ENABLE_FEATURE_FLAGS=true
SKIP_EMAIL_VERIFICATION=true

# Server Configuration
PORT=3000
API_BASE_URL=YOUR_API_BASE_URL

# AWS Configuration
AWS_REGION=SAMPLE_REGION
AWS_S3_BUCKET=YOUR_BUCKET

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY=YOUR_STRIPE_PUBLISHABLE_KEY
STRIPE_SECRET_KEY=YOUR_STRIPE_SECRET_KEY

# OneSignal Configuration
ONESIGNAL_APP_ID=YOUR_ONESIGNAL_APP_ID
ONESIGNAL_API_KEY=YOUR_ONESIGNAL_API_KEY

# Security Setting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
ENABLE_CORS_STRICT_MODE=false
ENABLE_HELMET_SECURITY=false

# AI API Keys
GOOGLE_GEMINI_API_KEY=YOUR_GOOGLE_GEMINI_API_KEY